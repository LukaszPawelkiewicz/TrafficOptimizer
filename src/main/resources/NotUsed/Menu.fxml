<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NotUsed.controller.MenuController">
   <children>
      <TableView fx:id="roadObjectTableView" layoutX="14.0" layoutY="12.0" prefHeight="376.0" prefWidth="309.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Road Object" />
          <TableColumn prefWidth="87.0" text="X" fx:id="xColumn" />
            <TableColumn fx:id="yColumn" prefWidth="75.0" text="Y" />
            <TableColumn fx:id="lengthColumn" prefWidth="75.0" text="Length" />
        </columns>
      </TableView>
      <Button fx:id="refreshButton" layoutX="530.0" layoutY="363.0" mnemonicParsing="false" onAction="#refreshAction" text="Refresh" />
      <Button fx:id="newObjectButton" layoutX="486.0" layoutY="14.0" mnemonicParsing="false" onAction="#newObjectAction" text="Add new object" />
   </children>
</AnchorPane>
